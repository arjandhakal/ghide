{
  "manifest_version": 3,
  "name": "Gemfold",
  "version": "1.0.0",
  "description": "Folders for Gemini",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle"
    }
  ],
  "permissions": ["storage"]
}
